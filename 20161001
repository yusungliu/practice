#include <iostream>
#include <algorithm>
using namespace std;

int a,b,c;
int input[3];

int main()
{
    while(cin>>input[0]>>input[1]>>input[2])
    {
        sort(input,input+3);
        a=input[0];
        b=input[1];
        c=input[2];
        for(int i=0;i<3;i++)
        {
            if(i==2)
                cout<<input[i]<<endl;
            else
                cout<<input[i]<<' ';
        }
        if(a+b<=c)
            cout<<"No"<<endl;
        else if(a*a+b*b==c*c)
            cout<<"Right"<<endl;
        else if(a*a+b*b<c*c)
            cout<<"Obtuse"<<endl;
        else if(a*a+b*b>c*c)
            cout<<"Acute"<<endl;
    }

    return 0;
}
